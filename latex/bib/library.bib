file = {:Users/bernardo/Library/Application Support/Mendeley Desktop/Downloaded/U.S. Department of Energy - 2010 - Buildings Sector Energy Consumption.pdf:pdf},
journal = {Building Energy Data Book},
pages = {111--126},
title = {{Buildings Sector Energy Consumption}},
