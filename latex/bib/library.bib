@article{Domingues2016,
abstract = {Following the widespread adoption of building management systems, driven mostly by building energy efficiency concerns, Building Automation has earned a great deal of attention. Indeed, an increasing number of stakeholders have put their efforts in the Building Automation market, developing devices and software solutions. It is well known that Building Automation technology is characterized by heterogeneity, in that solutions from different manufacturers cannot be easily integrated with each other, and that costumers are often forced to pick a single line of products, therefore, leading to costumer lock-in situations as well as increasing installation and maintenance costs. However, given the popularity of the subject, one startling aspect of Building Automation is the scarcity of authoritative literature references regarding the topic---definition inconsistency and disagreement are recurring issues. Moreover, automation technology specifications and manufacturer documentation are prevalently leading to a lack of coverage of the entire spectrum of functionality expected from modern automation systems. This results in communication difficulties between system developers further hindering the efforts to overcome heterogeneity. This article systematizes fundamental automation aspects, comprehensively defining each concept based upon established literature standards. Having these aspects as guidelines, we assess the required functionality of a Building Automation System and compare the main Building Automation specification standards available. We conclude that none of the analyzed specifications are able to totally cover the surveyed functionality span, leaving to developers and manufacturers the responsibility of creating custom extensions to fill functionality gaps, thus hindering the interoperability between different technologies.},
author = {Domingues, Pedro and Carreira, Paulo and Vieira, Renato and Kastner, Wolfgang},
doi = {10.1016/j.csi.2015.11.005},
file = {:Users/bernardo/Downloads/1-s2.0-S0920548915001361-main.pdf:pdf},
issn = {09205489},
journal = {Computer Standards {\&} Interfaces},
keywords = {Building automation,Building automation standards,Building automation technologies,Information models,Interoperability},
number = {November},
pages = {1--12},
publisher = {Elsevier B.V.},
title = {{Building automation systems: Concepts and technology review}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0920548915001361},
volume = {45},
year = {2016}
}
